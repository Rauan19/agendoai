# Configuração do Banco de Dados
DATABASE_URL=postgresql://username:password@localhost:5432/agendoai_db

# Configuração de Sessão
SESSION_SECRET=your-super-secret-session-key-here

# Configuração do Stripe (Opcional)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Configuração SendGrid (Opcional)
SENDGRID_API_KEY=SG.your_sendgrid_api_key_here

# Configuração Anthropic/Claude (Opcional)
ANTHROPIC_API_KEY=sk-ant-your_anthropic_api_key_here

# Configuração de Notificações Push (Opcional)
VAPID_PUBLIC_KEY=your_vapid_public_key_here
